spring.r2dbc.url=r2dbc:mariadb://localhost:3306/DATABASE_NAME
spring.r2dbc.username=USERNAME
spring.r2dbc.password=PASSWORD

spring.liquibase.url=jdbc:mariadb://localhost:3306/DATABASE_NAME
spring.liquibase.user=USERNAME
spring.liquibase.password=PASSWORD
spring.liquibase.change-log=db/changelog/db.changelog-master.xml

app.rate-limit.requests-allowed-per-period=100
app.rate-limit.period-millis=1000

spring.main.lazy-initialization=true